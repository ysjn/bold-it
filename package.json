{
  "name": "bold-it",
  "description": "Bold-it is a super simple plugin that bolds user-specified keyword in selected text layers.",
  "version": "1.0.0",
  "repository": "https://github.com/ysjn/bold-it.git",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "bold-it",
    "manifest": "src/manifest.json",
    "main": "bold-it.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4"
  },
  "author": "ysjn <@ys_jn>"
}
